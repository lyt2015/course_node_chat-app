{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
    "io": false
  },
  "rules": {
    "comma-dangle": ["warn", "always-multiline"],
    "linebreak-style": ["error", "windows"],
    "no-console": "off",
    "arrow-parens": ["error", "as-needed"],
    "no-unused-vars": [
      "error",
      {
        "vars": "local",
        "args": "none"
      }
    ],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-one-expression-per-line": 0,
    "semi": "off",
    "no-alert": "off",
    "consistent-return": "off"
  }
}
